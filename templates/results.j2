{% extends "main.j2" %}
{% block content %}

{# READ table #}
<table>
    <thead>
        {# For the table header row, we print the DB attribute names #}
        <tr>
            {% for key in results[0].keys() %}
            <th>{{ key }}</th>
            {% endfor %}
            <th></th>
        </tr>
    </thead>

    <tbody>
        {# For each row, print the testResultId, result #}
        {% for result in results %}
        <tr>
            <td>{{ result.testResultId }}</td>
            <td>{{ result.result }}</td>
            <td><button><span class="material-symbols-outlined">edit</span></button></td>
        </tr>
        {% endfor %}
    </tbody>
</table>

{% endblock %}